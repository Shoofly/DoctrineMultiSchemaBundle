{
	"name": "shoofly/doctrine-multi-schema-bundle",
	"description": "Enables multiple schemas for Doctrine + Symfony",
	"keywords": ["bundle", "symfony", "doctrine", "DBAL", "MySQL"],
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Sean Capaloff-Jones",
			"email": "sean@shooflysolutions.com",
			"role": "Creator"
	}],
	"support": {
		"email": "sean@shooflysolutions.com",
		"issues": "https://github.com/smcjones/doctrine-multi-schema-bundle/issues"
	},
    "require": {
		"php": ">=5.6.0",
        "doctrine/common": "~2.5",
        "doctrine/dbal": "~2.5",
        "symfony/config": "~3.2",
        "symfony/dependency-injection": "~3.2",
        "symfony/doctrine-bridge": "~3.2",
        "symfony/framework-bundle": "~3.2"
    },
	"require-dev": {
        "phpunit/phpunit": "5.7.*",
        "squizlabs/php_codesniffer": "~2.7",
        "escapestudios/symfony2-coding-standard": "~2.9"
	},
	"autoload": {
		"psr-4": {
			"Shoofly\\DoctrineMultiSchemaBundle\\": ""
		}
	}
}
